{
    "name": "Tabnabbing",
    "description" : "Highlighting the difference to prevent tabnabbing",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action":{
        "default_icon": {
            "16": "images/green-16.png",
            "32": "images/green.png",
            "48": "images/green.png",
            "128": "images/green.png"
        }
    },
    "background": {
        "page": "background.html",

        "persistent": true
    },
    "permissions": [
        "activeTab",
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval';"
}